@using GridMvc.Html
@model IEnumerable<WebProject.Models.Account.RegisterModel>
@{
    ViewBag.Title = "Completed tests";
}

@Html.Grid(Model).Columns(columns =>
{
    columns.Add(e => e.NumberOfCorrectAnswers).Titled("Correct Answers").Sortable(true);
    columns.Add(e => e.TestCompletionDate).Titled("Completed on").Sortable(true);
}).WithPaging(10)